<resources xmlns:tools="http://schemas.android.com/tools" tools:ignore="MissingTranslation">

    <!-- ShowcaseActivity Strings -->
    <string name="app_name">UX Icons</string>
    <string name="app_long_name">UX Icon Pack</string>
    <string name="theme_description">Hundreds of icons inspired by Samsungs UX icons.</string>

    <!-- Muzei Stuff -->
    <string name="artsource_name">@string/app_name</string>
    <string name="artsource_desc">UX Wallpapers</string>

    <string name="wallpapers_json_link">https://github.com/jahirfiquitiva/Website-Resources/raw/master/iconshowcase/iconshowcase.json</string>

    <!-- Icon Pack designer/owner e-mail -->
    <string name="email_id">hendrik.bloess@googlemail.com</string>

    <!-- Mail subjects for simple e-mail and requests e-mail -->
    <string name="email_subject">[Request] UX Icon Pack</string>
    <string name="request_title">UX Icon Request</string>

    <!-- This are the strings for the cards in the main screen -->
    <string name="welcome_title">Welcome to UX Icons</string>

    <!-- Strings for Wallpapers and Requests -->
    <string name="request_save_location">%1$s/UXIcons/Requests/</string>
    <string name="walls_save_location">%1$s/UXIcons/Wallpapers/</string>

    <!-- Launcher Activity Name
    if the activity is:
    com.packagename.appname.Activity
    then write Activity
    -->
    <string name="main_activity_name">HomeActivity</string>
    <string name="main_activity_fullname">com.hendrikbl.uxicons.HomeActivity</string>



    <!-- SOME INFO ABOUT YOU, TO BE USED IN THE APP AND SOME LAUNCHERS -->
    <string name="iconpack_author_name">Hendrik Blöß</string>
    <string name="iconpack_author_photo" formatted="false" tools:ignore="TypographyDashes">https://github.com/hendrikbl.png</string>
    <string name="iconpack_author_banner" formatted="false" tools:ignore="TypographyDashes">http://via.placeholder.com/350x150/09f/fff.jpg</string>
    <string name="iconpack_author_description">I like designing, playing games and watching movies.</string>
    <string name="iconpack_author_playstore">https://play.google.com/store/apps/dev?id=7438639276314720952</string>
    <string name="iconpack_author_website">https://www.jahirfiquitiva.me/</string>


    <!-- PICTURES NAMES TO BE USED AS PREVIEWS FOR LAUNCHERS -->

    <!-- Feature picture that appears above the description -->
    <string name="theme_feature">icons_banner</string>

    <!-- Previews Pictures Names -->
    <string name="theme_preview1">preview1</string>
    <string name="theme_preview2">preview2</string>

    <!-- These things are needed for some launchers (Feel free to edit, though is not needed) -->
    <string name="theme_title">@string/app_name</string>
    <string name="theme_name">@string/theme_title</string>
    <string name="developer_name">@string/iconpack_author_name</string>
    <string name="developerName">@string/developer_name</string>
    <string name="theme_author">@string/developer_name</string>
    <string name="authorName">@string/developer_name</string>
    <string name="authorLink">@string/iconpack_author_website</string>
    <string name="theme_info">@string/theme_description</string>
    <string name="theme_ad_msg">@string/theme_description</string>

</resources>